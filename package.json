{
  "name": "restincloud-node",
  "version": "2.1.0",
  "description": "Restincloud API for node",
  "main": "dist-obf/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist-obf",
    "dist/index.d.ts"
  ],
  "scripts": {
    "start": "npm run watch",
    "prepublish": "npm run build",
    "prebuild": "del dist/",
    "build": "tsc",
    "obfuscate": "npx javascript-obfuscator dist/ --output dist-obf/ --config obfuscator.config.json",
    "prepare": "npm run build && npm run obfuscate",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "firebase-functions": "^6.3.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/node-fetch": "^2.5.12",
    "cpy-cli": "^2.0.0",
    "del-cli": "^3.0.0",
    "javascript-obfuscator": "^4.1.1",
    "typescript": "^4.9.5"
  }
}
